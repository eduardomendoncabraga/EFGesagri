<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>EF-GesAgro - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/font-awesome.min.css")" type="text/css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <link href="@Url.Content("~/Content/style.css")" type="text/css" rel="stylesheet" media="all"/>
    <link href="@Url.Content("~/Content/styles.css")" type="text/css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet">
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container" id='cssmenu'>
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>

                </button>
                <a class="navbar-brand" href="index.html"><i class="fa fa-pagelines"></i>EF-GESAGRO</a>
            </div>
            <ul>
                <li class='active has-sub'><a href='#'>Cadastro funcionais</a>
                    <ul>
                        <li>@Html.ActionLink("Cadastro de proprietário", "Index", "Proprietario")</li>
                    </ul>
                </li>
                <li class='active has-sub'><a href='#'>Propriedades</a>
                    <ul>
                        <li>@Html.ActionLink("Cadastro de Talhão", "Index", "Talhao")</li>
                        <li>@Html.ActionLink("Cadastro de Fazendas", "Index", "Fazenda")</li>
                    </ul>
                </li>
                <li class='active has-sub'><a href='#'>Atividades rurais</a>
                    <ul>
                        <li>@Html.ActionLink("Culturas", "Index", "Cultura")</li>
                        <li>@Html.ActionLink("Variedades", "Index", "Variedade")</li>
                        <li>@Html.ActionLink("Safras", "Index", "Safra")</li>
                    </ul>
                </li>
                <li class='active has-sub'><a href='#'>Central de Custos</a>
                    <ul>
                        <li>@Html.ActionLink("Estimativa de custo", "Index", "CustoEstimado") </li>
                        <li>@Html.ActionLink("Custo realizado", "Index", "CustoRealizado")</li>
                    </ul>
                </li>
                <li class='active has-sub'><a href='#'>Relatórios</a>
                    <ul>
                        <li class='has-sub'><a href='#'>Custos</a>
                            <ul>
                                <li>@Html.ActionLink("Estimativa de custo", "RelarioCustoEstimado", "CustoEstimado", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                                <li>@Html.ActionLink("Custo realizado", "RelarioCustoRealizado", "CustoRealizado", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                                <li>@Html.ActionLink("CustoX", "CustEstXCustPrev", "Home", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                            </ul>
                        </li>
                        <li class='has-sub'><a href='#'>Propriedades</a>
                            <ul>
                                <li>@Html.ActionLink("Fazendas", "RelatorioFazenda", "Fazenda", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                                <li>@Html.ActionLink("Talhões", "RelatorioTalhao", "Talhao", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                                <li class='has-sub'>@Html.ActionLink("Proprietários", "RelatorioProprietario", "Proprietario", null, new { @class = "btn btn-xs btn-filter", @target = "_blank" })</li>
                            </ul>
                        </li>
                        <li class='has-sub'><a href='#'>Atividades rurais</a>
                            <ul>
                                <li>@Html.ActionLink("Safras", "RelatorioSafra", "Safra", null, new { @class = "btn btn-xs btn-filter", @target = "_blank"})</li>
                            </ul>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>
    <div class="container-fluid">
        @RenderBody()
    </div>
    <script src="@Url.Content("~/Scripts/jquery-2.1.4.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Table.js")"></script>
    <script src="@Url.Content("~/Scripts/Teste.js")"></script>
    <script src="@Url.Content("~/Scripts/responsiveslides.min.js")"></script>
    <script src="@Url.Content("~/Scripts/essenciais.js")"></script>
    <script src="@Url.Content("~/Scripts/Mascara.js")" type="text/javascript"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>

